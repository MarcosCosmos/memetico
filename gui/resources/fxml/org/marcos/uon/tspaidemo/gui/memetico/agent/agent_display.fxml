<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<fx:root type="Pane" xmlns:fx="http://javafx.com/fxml">
    <GridPane styleClass="dataGrid">
        <TextFlow GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="3" styleClass="agentDisplayTitle">
            <Text styleClass="lblAgentId">Agent&#160;</Text>
            <Text styleClass="txtAgentId"/>
        </TextFlow>
        <Label GridPane.rowIndex="1" GridPane.columnIndex="0" styleClass="colorLabel,pocketColorSample">&#160;</Label>
        <Text GridPane.rowIndex="1" GridPane.columnIndex="1">Pocket:</Text>
        <Text GridPane.rowIndex="1" GridPane.columnIndex="2" styleClass="txtPocketCost" textAlignment="RIGHT"/>
        <Label GridPane.rowIndex="2" GridPane.columnIndex="0" styleClass="colorLabel,currentColorSample,color">&#160;</Label>
        <Text GridPane.rowIndex="2" GridPane.columnIndex="1">Current:</Text>
        <Text GridPane.rowIndex="2" GridPane.columnIndex="2" styleClass="txtCurrentCost" textAlignment="RIGHT"/>
    </GridPane>
</fx:root>