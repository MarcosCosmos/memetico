<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.lang.*?>
<BorderPane fx:id="playbackControls" fx:controller="org.marcos.uon.tspaidemo.gui.main.PlaybackController"
            xmlns:fx="http://javafx.com/fxml">
    <center>
        <BorderPane fx:id="sliderWrapper">
            <left>
                <Text fx:id="txtMinFrame"/>
            </left>
            <center>
                <BorderPane>
                    <center>
                        <Text fx:id="txtCurFrame"/>
                    </center>
                    <top>
                        <Slider fx:id="sldrFrameIndex"/>
                    </top>
                </BorderPane>
            </center>
            <right>
                <Text fx:id="txtMaxFrame"/>
            </right>
        </BorderPane>
    </center>
    <right>
        <HBox>
            <Button fx:id="btnStop" text="â– " onAction="#stopPlayback"/>
            <Button fx:id="btnPlayPause" onAction="#togglePlayState">/</Button>
            <HBox>
                <Label fx:id="lblSpeed">Speed:</Label>
                <ChoiceBox fx:id="cbSpeed">
                    <Double fx:value="0.00138888888"/>
                    <Double fx:value="0.015" />
                    <Double fx:value="0.033" />
                    <Double fx:value="0.1" />
                    <Double fx:value="0.2" />
                    <Double fx:value="0.5" />
                    <Double fx:value="1" />
                    <Double fx:value="2" />
                    <Double fx:value="5" />
                    <Double fx:value="10" />
                    <Double fx:value="30" />
                    <Double fx:value="60" />
                </ChoiceBox>
            </HBox>
            <Text fx:id="txtTest"/>
        </HBox>
    </right>
</BorderPane>

